    <AssemblyName>Fuzzing.Manipulations</AssemblyName>
    <RootNamespace>Fuzzing.Manipulations</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>DotnetModelFuzzer.Manipulations</PackageId>
    <Version>0.0.2</Version>
    <Copyright>Zeebee Technologies 2021</Copyright>
    <Authors>Melissa Benua</Authors>
    <Company>Zeebee Technologies</Company>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

  
  <ItemGroup>
    <None Update="fuzzdb\**\*.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
